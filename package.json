{
  "name": "garbage-collector",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev -p 3001",
    "ny:build": "yarn netlify build",
    "ny:deploy": "yarn netlify deploy --prod",
    "lint": "eslint . --fix --max-warnings=0",
    "pretest": "rimraf .nyc_output || true",
    "test": "yarn test:unit && yarn test:e2e",
    "posttest": "yarn report:combined",
    "test:unit": "jest --passWithNoTests",
    "test:unit:watch": "jest --passWithNoTests --watch",
    "test:e2e": "start-test start http://localhost:3001 test:e2e:inner",
    "test:e2e:inner": "cypress run",
    "test:e2e:open": "start-test dev http://localhost:3001 test:e2e:open:inner",
    "test:e2e:open:inner": "cypress open --env failOnSnapshotDiff=false",
    "test:e2e:update-snapshot": "start-test start http://localhost:3001 test:e2e:update-snapshot:inner",
    "test:e2e:update-snapshot:inner": "cypress run --env updateSnapshots=true",
    "report:combined": "node ./scripts/combine-coverage-reports.js",
    "codegen": "yarn graphql-codegen && yarn graphql-typedefs",
    "graphql-typedefs": "node scripts/graphql-typedefs",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook --ci -p 9001",
    "check:storybook": "yarn storybook --smoke-test",
    "rearange:components": "yarn destiny"
  },
  "dependencies": {
    "@apollo/client": "3.3.11",
    "@apollo/link-context": "2.0.0-beta.3",
    "@cypress/code-coverage": "3.9.2",
    "@graphql-codegen/import-types-preset": "1.18.1",
    "@hookform/error-message": "0.0.5",
    "@hookform/resolvers": "2.0.0-rc.1",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "clsx": "1.1.1",
    "cookie": "0.4.1",
    "deepmerge": "4.2.2",
    "framer-motion": "3.6.7",
    "graphql": "15.5.0",
    "graphql-codegen-apollo-next-ssr": "1.4.0",
    "js-cookie": "2.2.1",
    "next": "10.0.7",
    "next-auth": "3.5.0",
    "next-translate": "1.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "6.15.3",
    "react-icons": "4.2.0",
    "react-use": "17.1.1",
    "react-window": "1.8.6",
    "uuid": "8.3.2",
    "yup": "0.32.9"
