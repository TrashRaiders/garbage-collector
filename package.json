    "lint-staged": "10.5.3",
    "next-compose-plugins": "2.2.1",
    "prettier": "2.2.1",
    "start-server-and-test": "1.12.0",
    "typescript": "4.1.3"
  },
  "resolutions": {
    "webpack": "4.46.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
