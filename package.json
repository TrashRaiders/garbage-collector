{
  "name": "garbage-collector",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev -p 3001",
    "lint": "eslint . --fix",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest --passWithNoTests",
    "test:e2e": "start-test start http://localhost:3001 test:e2e:inner",
    "test:e2e:inner": "cypress run",
    "test:e2e:open": "start-test dev http://localhost:3001 test:e2e:open:inner",
    "test:e2e:open:inner": "cypress open --env failOnSnapshotDiff=false",
    "test:e2e:update-snapshot": "start-test start http://localhost:3001 test:e2e:update-snapshot:inner",
    "test:e2e:update-snapshot:inner": "cypress run --env updateSnapshots=true",
    "codegen": "yarn graphql-codegen && yarn graphql-typedefs",
    "graphql-typedefs": "node scripts/graphql-typedefs",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook --ci -p 9001",
    "check:storybook": "yarn storybook --smoke-test",
    "rearange:components": "yarn destiny"
  },
  "dependencies": {
    "@apollo/client": "3.3.6",
    "@apollo/link-context": "2.0.0-beta.3",
    "@graphql-codegen/import-types-preset": "1.18.1",
    "@hookform/error-message": "0.0.5",
    "@hookform/resolvers": "2.0.0-rc.1",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "clsx": "1.1.1",
    "framer-motion": "3.2.1",
    "graphql": "15.5.0",
    "graphql-codegen-apollo-next-ssr": "1.3.0",
    "next": "10.0.5",
    "next-translate": "1.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "6.14.2",
    "react-select": "4.0.2",
    "react-use": "15.3.8",
    "react-window": "1.8.6",
    "uuid": "8.3.2",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@apollo/link-schema": "2.0.0-beta.3",
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.20.2",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@next/bundle-analyzer": "10.0.6",
    "@storybook/addons": "6.1.15",
    "@storybook/react": "6.1.15",
    "@testing-library/cypress": "7.0.3",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.2",
    "@twihike/eslint-config": "0.1.12",
    "@twihike/prettier-config": "0.1.12",
    "@types/cypress-image-snapshot": "3.1.4",
    "@types/graphql-iso-date": "3.4.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-window": "1.8.2",
    "@types/uuid": "8.3.0",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-import": "1.13.3",
    "babel-preset-react-app": "10.0.0",
    "critters": "0.0.6",
    "cross-env": "7.0.3",
    "cypress": "6.3.0",
    "cypress-image-snapshot": "4.0.1",
    "destiny": "0.7.1",
    "encoding": "0.1.13",
    "eslint": "7.19.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "27.0.0",
    "graphql-iso-date": "3.6.1",
    "graphql-tools": "5.0.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
